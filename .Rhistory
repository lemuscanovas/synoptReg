atm_data2 <- tidy_nc(x = list(mslp,z500),
time_subset = dates_int,
name_vars = c("mslp","z500"))
# S-mode classification
tmode_clas <- synoptclas(atm_data2, ncomp = 2, matrix_mode = "T-mode")
