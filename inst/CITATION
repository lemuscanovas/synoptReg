  author   = "Marc Lemus-Canovas and Joan A. Lopez-Bustins and Javier Martin-Vide and Dominic Royé",
  textVersion =
	paste("Lemus-Canovas, M., Lopez-Bustins, J.A., Martin-Vide, J., Roye., D., 2019. synoptReg: An R package for computing a synoptic climate classification and a spatial regionalization of environmental data.",
	"Environmental Modelling & Software, 118: 114-119.")
)
